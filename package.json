{
  "name": "react-ical-br",
  "version": "0.5.82-0",
  "description": "Infinite scrolling date-picker built with React, with localization, themes, keyboard support, and more.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "styles.css",
    "react-ical-br.d.ts"
  ],
  "scripts": {
    "build": "npm run clean && nwb build-react-component --no-vendor",
    "prepare": "npm run build",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "lint": "eslint --fix src/**/*.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "date-fns": "^1.27.2",
    "dom-helpers": "^3.2.1",
    "prop-types": "^15.5.7",
    "react-tiny-virtual-list": "^2.0.0",
    "react-transition-group": "^1.1.3",
    "recompose": "^0.22.0"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-options": "^1.0.1",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-css-modules-transform": "^1.2.1",
    "eslint": "^3.8.1",
    "eslint-config-react-app": "^0.5.1",
    "eslint-plugin-flowtype": "^2.21.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "node-sass": "^4.9.2",
    "nwb": "^0.21.5",
    "nwb-sass": "^0.8.4",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "infinite",
    "calendar",
    "endless",
    "date",
    "date-picker",
    "month",
    "day",
    "year",
    "scrolling",
    "virtual",
    "picker",
    "material",
    "flat"
  ],
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oflatley-br/react-ical-br.git"
  },
  "author": "oflatley",
  "bugs": {
    "url": "https://github.com/oflatley-br/react-ical-br/issues"
  }
}
